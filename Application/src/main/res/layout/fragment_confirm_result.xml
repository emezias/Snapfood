<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/confirm_result_fragment"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.snapfood.snapapp.MenuActivity"
    android:background="@android:color/background_light">

     <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
         android:measureWithLargestChild="true">
       <Button
            android:layout_width="42dp"
            android:layout_height="42dp"
            android:text="X"
            android:id="@+id/close_button"
            android:layout_gravity="right" />

        <TextView
            android:layout_width="fill_parent"
            android:layout_height="42dp"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:id="@+id/status_label"
            android:gravity="center_vertical|center_horizontal"
            android:text="passed" />

    </LinearLayout>


    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Bio"
        android:id="@+id/textView" />

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <TextView
            android:layout_width="128dp"
            android:layout_height="28dp"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:text="@string/first_name"
            android:id="@+id/first_name_label"
            android:gravity="bottom|right"
            android:paddingEnd="5dp" />

        <TextView
            android:layout_width="250dp"
            android:layout_height="28dp"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:id="@+id/first_name"
            android:layout_gravity="center_horizontal"
            android:gravity="bottom"
            android:textColor="@color/bright_foreground_material_light" />
    </LinearLayout>
    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <TextView
            android:layout_width="128dp"
            android:layout_height="28dp"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:text="@string/last_name"
            android:id="@+id/last_name_label"
            android:gravity="bottom|right"
            android:paddingEnd="5dp" />

        <TextView
            android:layout_width="250dp"
            android:layout_height="28dp"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:id="@+id/last_name"
            android:layout_gravity="center_horizontal"
            android:gravity="bottom"
            android:textColor="@color/bright_foreground_material_light" />
    </LinearLayout>
    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <TextView
            android:layout_width="128dp"
            android:layout_height="28dp"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:text="@string/address"
            android:id="@+id/address_label"
            android:gravity="bottom|right"
            android:paddingEnd="5dp" />

        <TextView
            android:layout_width="250dp"
            android:layout_height="28dp"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:id="@+id/address"
            android:layout_gravity="center_horizontal"
            android:gravity="bottom"
            android:textColor="@color/bright_foreground_material_light" />
    </LinearLayout>
    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <TextView
            android:layout_width="128dp"
            android:layout_height="28dp"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:text="@string/city"
            android:id="@+id/city_label"
            android:gravity="bottom|right"
            android:paddingEnd="5dp" />

        <TextView
            android:layout_width="250dp"
            android:layout_height="28dp"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:id="@+id/city"
            android:layout_gravity="center_horizontal"
            android:gravity="bottom"
            android:textColor="@color/bright_foreground_material_light" />
    </LinearLayout>
    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <TextView
            android:layout_width="128dp"
            android:layout_height="28dp"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:text="@string/state"
            android:id="@+id/state_label"
            android:gravity="bottom|right"
            android:paddingEnd="5dp" />

        <TextView
            android:layout_width="250dp"
            android:layout_height="28dp"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:id="@+id/state"
            android:layout_gravity="center_horizontal"
            android:gravity="bottom"
            android:textColor="@color/bright_foreground_material_light" />
    </LinearLayout>
    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <TextView
            android:layout_width="128dp"
            android:layout_height="28dp"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:text="@string/zip"
            android:id="@+id/zip_label"
            android:gravity="bottom|right"
            android:paddingEnd="5dp" />

        <TextView
            android:layout_width="250dp"
            android:layout_height="28dp"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:id="@+id/zip"
            android:layout_gravity="center_horizontal"
            android:gravity="bottom"
            android:textColor="@color/bright_foreground_material_light" />
    </LinearLayout>
    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <TextView
            android:layout_width="128dp"
            android:layout_height="28dp"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:text="@string/dob"
            android:id="@+id/dob_label"
            android:gravity="bottom|right"
            android:paddingEnd="5dp" />

        <TextView
            android:layout_width="250dp"
            android:layout_height="28dp"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:id="@+id/dob"
            android:layout_gravity="center_horizontal"
            android:gravity="bottom"
            android:textColor="@color/bright_foreground_material_light" />
    </LinearLayout>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Classification"
        android:id="@+id/textView2" />

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <TextView
            android:layout_width="128dp"
            android:layout_height="28dp"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:text="@string/iss_type"
            android:id="@+id/iss_type_label"
            android:gravity="bottom|right"
            android:paddingEnd="5dp" />

        <TextView
            android:layout_width="250dp"
            android:layout_height="28dp"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:id="@+id/iss_type"
            android:layout_gravity="center_horizontal"
            android:gravity="bottom"
            android:textColor="@color/bright_foreground_material_light" />
    </LinearLayout>
    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <TextView
            android:layout_width="128dp"
            android:layout_height="28dp"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:text="@string/iss_state"
            android:id="@+id/iss_state_label"
            android:gravity="bottom|right"
            android:paddingEnd="5dp" />

        <TextView
            android:layout_width="250dp"
            android:layout_height="28dp"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:id="@+id/iss_state"
            android:layout_gravity="center_horizontal"
            android:gravity="bottom"
            android:textColor="@color/bright_foreground_material_light" />
    </LinearLayout>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Issuance"
        android:id="@+id/textView3" />
    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <TextView
            android:layout_width="128dp"
            android:layout_height="28dp"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:text="@string/number"
            android:id="@+id/number_label"
            android:gravity="bottom|right"
            android:paddingEnd="5dp" />

        <TextView
            android:layout_width="250dp"
            android:layout_height="28dp"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:id="@+id/number"
            android:layout_gravity="center_horizontal"
            android:gravity="bottom"
            android:textColor="@color/bright_foreground_material_light" />
    </LinearLayout>
    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <TextView
            android:layout_width="128dp"
            android:layout_height="28dp"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:text="@string/issued"
            android:id="@+id/issued_label"
            android:gravity="bottom|right"
            android:paddingEnd="5dp" />

        <TextView
            android:layout_width="250dp"
            android:layout_height="28dp"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:id="@+id/issued"
            android:layout_gravity="center_horizontal"
            android:gravity="bottom"
            android:textColor="@color/bright_foreground_material_light" />
    </LinearLayout>
    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <TextView
            android:layout_width="128dp"
            android:layout_height="28dp"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:text="@string/expiration"
            android:id="@+id/expiration_label"
            android:gravity="bottom|right"
            android:paddingEnd="5dp" />

        <TextView
            android:layout_width="250dp"
            android:layout_height="28dp"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:id="@+id/expiration"
            android:layout_gravity="center_horizontal"
            android:gravity="bottom"
            android:textColor="@color/bright_foreground_material_light" />
    </LinearLayout>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Server"
        android:id="@+id/server_header" />
    <LinearLayout
        android:id="@+id/failure_layout"
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <TextView
            android:layout_width="128dp"
            android:layout_height="28dp"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:text="@string/failure"
            android:id="@+id/failure_label"
            android:gravity="bottom|right"
            android:paddingEnd="5dp" />

        <TextView
            android:layout_width="250dp"
            android:layout_height="28dp"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:id="@+id/failure"
            android:layout_gravity="center_horizontal"
            android:gravity="bottom"
            android:textColor="@color/bright_foreground_material_light" />
    </LinearLayout>

</LinearLayout>
